universe = docker
docker_image = docker-registry.champalimaud.pt/ants:61cbb853de44b7a1325682bba7fc8b9d0f4b0078
executable = ants_run.sh
arguments = ants.py

# File transfer
transfer_input_files = ants.py

# Change according to your app needs
request_cpus = 12
request_memory = 62000
request_gpus = 0

# Logging
stream_output = True
output = output_$(Cluster)-$(Process).txt
error = error_$(Cluster)-$(Process).txt
log = log_$(Cluster)-$(Process).txt

queue
